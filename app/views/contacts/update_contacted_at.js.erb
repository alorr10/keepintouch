$(".contact<%= @contact.id%>").html("<%= should_contact(@contact) %>");
$(".contact<%= @contact.id%>").css("background-color", "#a5ffc6");

window.location.href = "<%= contact(@contact) %>";

<% @user.contacts.order("last_contacted_on desc").each do |c| %>
  $(".contacts-list").replace_with("<%= j render c %>");
<% end %>